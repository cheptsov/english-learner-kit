{
    "manifest_version": 2,

    "name": "English Learner Kit",
    "description": "This extension helps learn phonetic transcription and pronunciation.",
    "version": "1.0.10",

    "permissions": [
        "https://dictionary.yandex.net/",
        "storage",
        "notifications",
        "background"
    ],

    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "css": ["style.css"],
            "js": ["jquery.js", "content.js"]
        }
    ],

    "background": {
        "scripts": ["background.js"]
    }
}
